<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag_configuracao"
    tools:context="com.gmail.thales_silva_nascimento.alarmmed.activity.ConfiguracaoActivity">

    <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/tbConfiguracao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        app:theme="@style/ThemeOverlay.AppCompat.Dark"/>

    <RelativeLayout
        android:id="@+id/rlConfiguracao"
        android:layout_below="@+id/tbConfiguracao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </RelativeLayout>


</RelativeLayout>
